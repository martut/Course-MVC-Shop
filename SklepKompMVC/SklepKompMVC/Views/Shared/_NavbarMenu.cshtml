@model SklepKompMVC.ViewModel.HomeViewModel
@using SklepKompMVC.Infrastructure

<ul class="dropdown-menu">
    @foreach (var item in Model.Categories)
    {
        <li>
            <a @*style="background: no-repeat ; background-image: url(@Url.CategoryIconsPath(item.CatIconFileName));"*@
               href="@Url.Action("List","Store", new { categoryname = item.CategoryName.ToLower() } )">
                @item.CategoryName
            </a>
        </li>
    }
</ul>